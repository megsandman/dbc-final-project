{
  "manifest_version": 2,

  "name": "ChefBoard",
  "description": "Save recipe link and details to chefboard",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://localhost:3000/",
    "https://chefboard.herokuapp.com/",
    "https://chefboard.herokuapp.com/*",
    "http://chefboard.herokuapp.com/",
    "http://chefboard.herokuapp.com/*"
  ]
}